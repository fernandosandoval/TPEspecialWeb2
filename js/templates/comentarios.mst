{{#comentarios}}
<tr>
    <td idrow="{{id_comentario}}">{{id_comentario}}</td>
    <td>{{nombre}}</td>
    <td>{{texto}}</td>
    <td>{{usuario}}</td>
    <td>{{puntaje}}</td>
    <td><a class="js-borrar" data-idcomentario="{{id_comentario}}"><span class="glyphicon glyphicon-trash" aria-hidden="true"></span></a></td>
</tr>
{{/comentarios}}
